<script>
  document.title = "{{ meta.title }} - JakeArchibald.com";
</script>

<div class="article-content">
  <h1>{{ meta.title }}</h1>
  <time class="article-date" datetime="{{ meta.posted | date('YYYY-MM-DD') }}">
    Posted {{ meta.posted | date("D MMM YYYY") }}
  </time>
  {{ content|safe }}
</div>